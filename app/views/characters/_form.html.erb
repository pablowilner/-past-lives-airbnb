 <%= simple_form_for [ @character] do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :name %>
        <%= f.input :profession %>
        <%= f.input :historical_period %>
        <%= f.input :location %>
        <%= f.input :price %>
        <%= f.input :photo, as: :file %>
        <%= f.submit "Submit ", class: "btn btn-primary" %>
<%  end %>
